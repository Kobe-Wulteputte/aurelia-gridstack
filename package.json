{
  "name": "@aurelia-gridstack/monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/aurelia-gridstack",
    "packages/demo"
  ],
  "scripts": {
    "build": "cd packages/aurelia-gridstack && npm run build",
    "start": "cd packages/demo && npm start",
    "changelog": "cross-env conventional-changelog -p angular -i docs/CHANGELOG.md -s",
    "deploy": "cd packages/demo && npm run build:demo && cd ../.. && deploy.sh",
    "publish:latest": "cd packages/aurelia-gridstack && npm run publish"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-history-browser": "^1.4.0",
    "aurelia-logging-console": "^1.1.1",
    "aurelia-templating-binding": "^1.5.3",
    "aurelia-templating-resources": "^1.13.1",
    "aurelia-templating-router": "^1.4.0",
    "aurelia-typed-observable-plugin": "^0.4.2",
    "aurelia-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.23.4",
    "gridstack": "^4.2.5",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "postcss-loader": "^4.0.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.1.0",
    "tslib": "^2.1.0",
    "typescript": "^4.3.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
